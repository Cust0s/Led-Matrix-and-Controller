void drawPicture(unsigned long frame[], int mySize){

  int ledIndex = 0;
  int xOffset = 0;    //the offset to be added for each  column
  int yOffset = 0;    //the offset to be added for each row
  
  for(int i = 0; i < 4; i++){
    
    switch(i){
      case 0:
        
        break;
      case 1:
        xOffset = 10;
        break;

      case 2:
        xOffset = 0;
        yOffset = 200;
        break;

      case 3:
        xOffset = 10;
        break;
    }
    for(int row = 0; row < 10; row++){
      for(int col = 0; col < 10; col++){
        int currentIndex = row * 20 + yOffset + col + xOffset;
        //Serial.println(currentIndex);
        

        leds[ledIndex].red = frame[currentIndex] >> 16 ;
    
        leds[ledIndex].green = (frame[currentIndex] & 0x00ff00) >> 8;
        
        leds[ledIndex].blue = (frame[currentIndex] & 0x0000ff);
        ledIndex++;
      }
    }
  }
}

